<h1>Books</h1>
<table>
 <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.title %></td>
        <td><%= book.body %></td>
        <td><a class="show_38285" href="/books/38285">Show</a></td>
        <td><a class="edit_38285" href="/books/38285/edit">Edit</a></td>
        <td><a data-confirm="Are you sure?" class="destroy_38285" rel="nofollow" data-method="delete" href="/books/38285">Destroy</a></td>
      </tr>
    <% end %>
  </tbody>
</table>
<h2>New book</h2>
<%= form_with model:@book, local:true do |f| %>

  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Body</h4>
  <%= f.text_field :body %>

  <%= f.submit 'CreateBooks' %>

<% end %>